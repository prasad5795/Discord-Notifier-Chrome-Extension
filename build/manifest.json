{
  "name": "Discord Notifier",
  "version": "1.0",
  "manifest_version": 2,
  "author": "Prasad Kulkarni<kulkarniprasad57@gmail.com>",
  "content_scripts": [
    {
      "matches": ["https://discord.com/*"],
      "js": ["scripts/content.js"]
    }
  ],
  "permissions": ["tabs", "storage"],
  "background": {
    "scripts": ["scripts/background.js"]
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icons/discord_16x16.png",
    "48": "icons/discord_48x48.png",
    "128": "icons/discord_128x128.png"
  }
}
